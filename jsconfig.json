{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "components/*": [
        "src/components/*"
      ],
      "constants/*": [
        "src/constants/*"
      ],
      "data/*": [
        "src/data/*"
      ],
      "navigation/*": [
        "src/navigation/*"
      ],
      "reducers/*": [
        "src/reducers/*"
      ],
      "lib/*": [
        "src/lib/*"
      ],
      "i18n/*": [
        "src/i18n/*"
      ],
      "views/*": [
        "src/views/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}